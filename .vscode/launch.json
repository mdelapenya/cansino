    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["chase"]
        },
        {
            "name": "Get One day",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "-d", "2020-01-23"]
        },
        {
            "name": "Get Today",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "-d", "Today"]
        }
    ]
}