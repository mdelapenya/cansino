    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["chase"]
        },
        {
            "name": "Get Since",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "-s", "2020-10-29"]
        },
        {
            "name": "Get Since Region",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "-s", "2020-10-29", "-r", "Castilla-La Mancha"]
        },
        {
            "name": "Get Today",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "--since", "Today"]
        },
        {
            "name": "Get Today For Region",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/docker/.env",
            "args": ["get", "--since", "Today", "-r", "Castilla-La Mancha"]
        }
    ]
}